{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "import-name": [
            false
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-pascal-case"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false,
        "max-line-length": [
            true,
            {
                "limit": 150
            }
        ]
    },
    "rulesDirectory": []
}